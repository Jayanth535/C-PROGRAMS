#include <stdio.h>

int main(void) {
	int ans=0;
    int n1,n2,n3,n4;
    scanf("%d%d%d%d",&n1,&n2,&n3,&n4);
    if(n1>9){
            ans+=1;
        }
    if(n2>9){
            ans+=1;
        }
    if(n3>9){
            ans+=1;
        }
    if(n4>9){
            ans+=1;
        }
    printf("\n%d",ans);
}

